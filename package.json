{
  "name": "@loladb/sdk-js",
  "version": "1.0.14",
  "description": "LolaDB Node.JS js/ts sdk",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-ts": "tsc -p ./tsconfig.json",
    "changelog": "conventional-changelog -p node -i CHANGELOG.md -s",
    "lint": "eslint . --ext .ts",
    "release": "npm run build && bumpp -x \"npm run changelog\" --all --commit --tag --push && npm publish",
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsup src/index.ts --format cjs,esm,iife --dts --clean",
    "dev": "npm run clean && unbuild --stub",
    "tests": "jest --ci --verbose ./src/__tests__/**/*.tsx --forceExit",
    "test": "jest",
    "watch": "npm run build -- --watch src"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loladb/sdk-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/loladb/sdk-js/issues"
  },
  "homepage": "https://github.com/loladb/sdk-js#readme",
  "devDependencies": {
    "@swc/core": "^1.3.2",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.23.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsonc": "^2.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "^29.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "dependencies": {
    "@types/node": "^18.7.18",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2"
  }
}
